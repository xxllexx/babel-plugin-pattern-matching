{
  "name": "babel-plugin-pattern-matching",
  "version": "1.4.3",
  "description": "Transform 'haskell like' pattern matching. Syntax analyzing based on bitwise operators like |(OR) and &(AND). Transforms to javascript function that comes with plugin as a helpers library.",
  "main": "lib/index.js",
  "author": "xxllexx",
  "repository": "https://github.com/xxllexx/babel-plugin-pattern-matching",
  "scripts": {
    "build": "babel src --out-dir lib --presets es2015,stage-2",
    "prepublish": "npm run build",
    "test": "mocha --check-leaks --compilers js:babel-core/register"
  },
  "devDependencies": {
    "async": "^2.1.5",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.1.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "lodash-bound": "^1.1.2",
    "lodash-compat": "^3.10.2",
    "lodash-es": "^4.17.4",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "babel-template": "^6.23.0",
    "glob": "^7.1.1"
  }
}
